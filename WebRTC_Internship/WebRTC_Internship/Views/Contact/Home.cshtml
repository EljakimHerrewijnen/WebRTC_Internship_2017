@model IEnumerable<WebRTC_Internship.Models.ContactModel>
@{
    ViewData["Title"] = "Home";
}
<!DOCTYPE html>
<html>
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script src="~/js/Contact/Home.js"></script>
    <link href="@Url.Content("~/css/Home.css")" type="text/css" rel="stylesheet" />
</head>

<body>
    <div id="SEARCH_area"><button id="SEARCH_viewbutton" type="button" onclick="SEARCH_view()">Search contacts</button></div>
    <div id="Contacts-list-container">
    </div>
    <div id="Contacts-list-container2">
    </div>


    <!-- The Modal -->
    <div id="myModal" class="modal">

        <!-- Modal content -->
        <div class="modal-content">
            <span class="close">&times;</span>
            <p>Incomming call!</p>
            <button onclick="CALL_Accepted()">Answer Call</button>
            <button onclick="CALL_Rejected()">Reject Call</button>
        </div>
    </div>
    <form method="post" enctype="multipart/form-data" asp-controller="Contact" asp-action="Uploadfile">
        <div class="form-group">
            <div class="col-md-10">
                <p>Upload one or more files using this form:</p>
                <input type="file" name="files" multiple accept="img/jpg" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Upload" />
            </div>
        </div>
    </form>
</body>
</html>
